{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-standard",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules" : {
        "ordered-imports": false,
        "object-literal-key-quotes" : false,
        "member-ordering": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false
    },
    "rulesDirectory": []
}